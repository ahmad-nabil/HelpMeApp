<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Framelayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
android:layout_gravity="center_vertical"
    tools:context=".register.signup">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/typeAccTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/bungeeregular"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/textView6"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintBottom_toTopOf="@id/textInputLayout"/>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="10dp"
            android:textColorHint="#E10707"
            app:hintEnabled="true"
            app:layout_constraintTop_toBottomOf="@id/typeAccTitle"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <EditText
                android:id="@+id/FullName_ET_signup"
                style="@android:style/Widget.AutoCompleteTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#EBEC80"
                android:ems="10"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="Full name"
                android:inputType="text"
                android:letterSpacing=".15"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textSize="16sp" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="10dp"
            android:textColorHint="#E10707"
            app:boxStrokeColor="#7A1313"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout">

            <EditText
                android:id="@+id/Email_ET_signup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EBEC80"
                android:ems="15"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="Email"
                android:inputType="textEmailAddress"
                android:letterSpacing=".15"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textSize="16sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="10dp"
            android:fitsSystemWindows="true"
            android:textColorHint="#E10707"
            app:boxStrokeColor="#7A1313"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout3"
            app:passwordToggleEnabled="true">

            <EditText
                android:id="@+id/Password_ET_signup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EBEC80"
                android:ems="10"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="password"
                android:inputType="textPassword"
                android:letterSpacing=".15"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textSize="16sp" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="10dp"
            android:textColorHint="#E10707"
            app:boxStrokeColor="#7A1313"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout6">

            <EditText
                android:id="@+id/LevelExperience"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EBEC80"
                android:ems="10"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="Level of experience"
                android:inputType="numberSigned"
                android:letterSpacing=".25"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textSize="16sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="10dp"
            android:textColorHint="#E10707"
            app:boxStrokeColor="#7A1313"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout5">

            <EditText
                android:id="@+id/TypeExperience"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EBEC80"
                android:ems="10"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="Type of experience"
                android:inputType="text"
                android:letterSpacing=".15"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textSize="16sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="10dp"
            android:textColorHint="#E10707"
            app:boxStrokeColor="#7A1313"
            app:hintTextColor="#E10707"
            app:layout_constraintBottom_toTopOf="@+id/textInputLayout8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout4">

            <TextView
                android:id="@+id/getLocation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EBEC80"
                android:clickable="true"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="Location"
                android:letterSpacing=".15"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textColorHint="#E10707"
                android:textSize="16sp"
                app:drawableRightCompat="@drawable/baseline_add_location_alt_24" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="30dp"
            android:textColorHint="#E10707"
            app:boxStrokeColor="#7A1313"
            app:layout_constraintBottom_toTopOf="@+id/registerBtn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout7">

            <EditText
                android:id="@+id/phoneEt_signup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:autoSizeTextType="none"
                android:autoText="false"
                android:background="#EBEC80"
                android:ems="10"
                android:fontFamily="@font/bungeeregular"
                android:gravity="center"
                android:hint="phone number"
                android:importantForAutofill="no"
                android:inputType="phone"
                android:letterSpacing=".15"
                android:padding="5dp"
                android:textColor="#E10707"
                android:textColorHint="#E10707"
                android:textSize="16sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <Button
            android:id="@+id/registerBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:background="@color/relativeTransparent"
            android:fontFamily="@font/bungeeregular"
            android:text="Login"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout8" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/bungeeregular"
            android:gravity="center"
            android:text="Help Me APP"
            android:textColor="#E10707"
            android:textSize="30sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>